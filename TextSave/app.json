{
  "expo": {
    "name": "TextSave",
    "slug": "Text",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Sanswet"
    },
    "android": {
      "package": "com.Sanswet.ReactNative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
